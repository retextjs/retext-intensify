{
  "name": "retext-intensify",
  "version": "2.0.0",
  "description": "Check for weak and mitigating wording with retext",
  "license": "MIT",
  "keywords": [
    "simple",
    "simpler",
    "synonym",
    "alternative",
    "phrase",
    "retext",
    "plugin"
  ],
  "repository": "https://github.com/wooorm/retext-intensify",
  "bugs": "https://github.com/wooorm/retext-intensify/issues",
  "author": "Titus Wormer <tituswormer@gmail.com> (http://wooorm.com)",
  "contributors": [
    "Titus Wormer <tituswormer@gmail.com> (http://wooorm.com)"
  ],
  "files": [
    "index.js"
  ],
  "dependencies": {
    "arr-union": "^3.0.0",
    "array-differ": "^1.0.0",
    "fillers": "^1.0.0",
    "hedges": "^1.0.0",
    "nlcst-search": "^1.0.0",
    "nlcst-to-string": "^2.0.0",
    "quotation": "^1.0.0",
    "unist-util-position": "^3.0.0",
    "weasels": "^1.0.0"
  },
  "devDependencies": {
    "browserify": "^14.0.0",
    "esmangle": "^1.0.1",
    "nyc": "^10.0.0",
    "remark-cli": "^2.1.0",
    "remark-preset-wooorm": "^2.0.0",
    "retext": "^4.0.0",
    "tape": "^4.0.0",
    "xo": "^0.17.1"
  },
  "scripts": {
    "build-md": "remark . --quiet --frail",
    "build-bundle": "browserify index.js --bare -s retextIntensify > retext-intensify.js",
    "build-mangle": "esmangle retext-intensify.js > retext-intensify.min.js",
    "build": "npm run build-md && npm run build-bundle && npm run build-mangle",
    "lint": "xo",
    "test-api": "node test",
    "test-coverage": "nyc --reporter lcov tape test.js",
    "test": "npm run build && npm run lint && npm run test-coverage"
  },
  "nyc": {
    "check-coverage": true,
    "lines": 100,
    "functions": 100,
    "branches": 100
  },
  "xo": {
    "space": true,
    "ignores": [
      "retext-intensify.js"
    ]
  },
  "remarkConfig": {
    "output": true,
    "presets": "wooorm"
  }
}
